{
  "name": "engagement-lab-website-2.x",
  "version": "2.5.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "qa": "ng serve --port 8000 --host 0.0.0.0 --disableHostCheck true",
    "build": "ng build --configuration=production",
    "build:qa": "ng build --configuration=qa --progress",
    "build:static": "npm-run-all -p serve:api:prod scully:build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "supportedBrowsers": "echo \"module.exports = $(browserslist-useragent-regexp --allowHigherVersions);\" > supported-browsers.js",
    "kill-node": "killall node",
    "dev": "npm-run-all -p serve:api start",
    "dev-qa": "npm-run-all -p serve:api qa",
    "serve:a11y": "pm2 start `which http-server` -- ../bin/app -p 1864 -b",
    "serve:api": "cd ../api; nodemon;",
    "serve:api:prod": "cd ../api; nodemon prod;",
    "scully": "scully",
    "scully:build": "scully --sr --RSD --sge",
    "scully:serve": "scully serve",
    "postinstall": "ngcc"
  },
  "private": false,
